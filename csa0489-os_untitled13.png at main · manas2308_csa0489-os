.hover-grow:hover, .anim-hover-grow:hover { transform: scale(1.025); }

.anim-rotate { animation: 1s linear 0s infinite normal none running rotate-keyframes; }

@keyframes rotate-keyframes { 
  100% { transform: rotate(360deg); }
}

